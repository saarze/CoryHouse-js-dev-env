{
  "name": "javascript-development-environment",
  "side-effect": [
    "*.css"
  ],
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js && babel-node buildScripts/start.js",
    "dev-server": "webpack-dev-server --mode development --open --color",
    "start": "npm-run-all --parallel dev-server lint-w",
    "share": "npm-run-all --parallel localTunnel start",
    "localTunnel": "babel-node buildScripts/shareMessage.js && babel-node buildScripts/share.js && lt --port 1998 --subdomain yash",
    "clean": "rm -r dist/*",
    "build": "webpack --mode development",
    "prod-build": "webpack --mode production",
    "lint": "esw webpack.config.* src --color",
    "lint-w": "npm run lint -- --watch"
  },
  "author": "Yash Handa",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "@babel/node": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "csv-loader": "^3.0.2",
    "eslint": "^5.7.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-watch": "^4.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^12.2.0",
    "json-schema-faker": "^0.5.0-rc15",
    "json-server": "^0.14.0",
    "localtunnel": "^1.9.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "nock": "^10.0.1",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-md5-hash": "0.0.6",
    "xml-loader": "^1.2.1"
  }
}
